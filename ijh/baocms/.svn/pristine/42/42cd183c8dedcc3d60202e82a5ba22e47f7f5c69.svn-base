$(function () {
	$('.sy_hottjTab li').each(function(e){
		$(this).hover(function(){
			$(".sy_hottjTab li").removeClass("on");
			$(this).addClass("on");
			$(".sy_hottj").each(function(i){
				if(e==i){
					$(".sy_hottj").hide();
					$(this).show();
				}
				else{
					$(this).hide();
				}
			});
		});
	});
});//首页js
