    <script>
		$(function(){
			$(".sh_micro_tab li").each(function(e){
				$(this).click(function(){
					$(this).parent().find("li").removeClass("on");
					$(this).addClass("on");
					$(".sh_micro_tab_box").each(function(i){
						if(e==i){
							$(".sh_micro_tab_box").hide();
							$(this).show();
						}
						else{
							$(this).hide();
						}
						if(e!==0){
							$(".sh_search_box .sh_search_int").hide();
							$(".sh_search_box .sh_search_more").show();
						}
						else{
							$(".sh_search_box .sh_search_int").show();
							$(".sh_search_box .sh_search_more").hide();
						}
						if(e==2 || e==3){
							$(".sh_micro_footer_box").show();
						}
						else{
							$(".sh_micro_footer_box").hide();
						}
						if(e==4){
							$(".sh_prompt_infor_box").show();
						}
						else{
							$(".sh_prompt_infor_box").hide();
						}
					});
				});
			});
		});
