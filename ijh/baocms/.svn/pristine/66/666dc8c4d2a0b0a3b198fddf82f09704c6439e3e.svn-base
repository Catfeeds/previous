/* CSS Document */
/*共用样式开始*/
header{height: 0.5rem; line-height: 0.5rem; background: #1ec0be; color: #FFF; position:fixed; width: 100%; z-index: 999; top: 0;}
header .title{font-size: 0.16rem; text-align: center;}
header i{ position:absolute; display:inline-block; font-style:normal;}
header i a{ font-size:0.14rem; color:#fff; line-height:0.5rem;}
header .left{ left:0.1rem; top:0;}
header .right{ right:0.1rem; top:0;}

.menuIco{background:url(../images/headerIco.png) no-repeat center -1.44rem;background-size:100% auto; padding:0.12rem; font-size:0; vertical-align:middle;}
.gobackIco{background:url(../images/headerIco.png) no-repeat center top;background-size:100% auto; padding:0.12rem; font-size:0; vertical-align:middle;}
.searchIco{background:url(../images/headerIco.png) no-repeat center -0.24rem;background-size:100% auto; padding:0.12rem; font-size:0; vertical-align:middle;}
.delIco{background:url(../images/headerIco.png) no-repeat center -0.48rem;background-size:100% auto; padding:0.12rem; font-size:0; vertical-align:middle;}
.bellIco{background:url(../images/headerIco.png) no-repeat center -0.72rem;background-size:100% auto; padding:0.12rem; font-size:0; vertical-align:middle; position:relative;}
.bellIco .num{ display:inline-block; width:0.14rem; height:0.14rem; border-radius:100%; position:absolute; top:0; right:0; text-align:center; color:#fff; font-size:0.12rem; line-height:0.14rem; background-color:#ff2121;}
.addrIco{background:url(../images/headerIco.png) no-repeat center -0.96rem;background-size:100% auto; padding:0.12rem; font-size:0; vertical-align:middle;}
.downIco{content:''; border-left: 0.05rem solid transparent; border-right: 0.05rem solid transparent; border-top: 0.06rem solid #fff; vertical-align:middle; font-size:0;}

.page_center_box{position: absolute; top: 0.5rem; padding-bottom:0.7rem; width: 100%;}

footer{height: 0.6rem;border-top: 0.01rem #dedede solid; position: fixed; bottom: 0; z-index: 999; background: #fff; width: 100%;}
footer .list{width:25%; float: left; text-align:center;}
footer .list a{ display:block; padding-top:0.08rem;}
footer .list a i{display:inline-block; background:url(../images/footerIco.png) no-repeat; background-size:0.5rem auto; width:0.25rem; height:0.25rem; vertical-align:middle;}
footer .list a i.ico_1{ background-position:0 0;}
footer .list a i.ico_2{ background-position:0 -0.25rem;}
footer .list a i.ico_3{ background-position:0 -0.5rem;}
footer .list a i.ico_4{ background-position:0 -0.75rem;}
footer .list.on a i{ background-position-x:-0.25rem;}
/*共用样式结束*/

/*头部商圈部分开始*/
header .shangquan{ font-size:0.16rem; color:#fff; display:inline-block;}
header .shangquan .addrIco{ background-position:center -0.88rem; padding:0.11rem; margin-right:0.02rem;}
header .shangquan .downIco{ margin-left:0.05rem;}
/*头部商圈部分结束*/

/*重复使用样式开始*/
/*下拉选项*/
#downOption{ border-bottom:0.01rem solid #dedede; border-top:0.01rem solid #dedede; background:#fff; padding:0.05rem 0; position:fixed; z-index:999; top:0.5rem; left:0; width:100%;}
#downOption .list{ display:block; float:left; width:33.33%; text-align:center; font-size:0.14rem; line-height:0.3rem; position:relative;}
#downOption .list a{display:block; border-right:0.01rem solid #dedede; font-size:0.14rem; line-height:0.3rem; color:#333;}
#downOption .list:last-child a{border-right:none 0;}
#downOption .list a em{content:''; display:inline-block; border-left: 0.04rem solid transparent; border-right: 0.04rem solid transparent; border-top: 0.05rem solid #666; vertical-align:middle; font-size:0; margin-left:0.05rem;}
#downOption .list.on a{ color:#1ec0be;}
#downOption .list.on a em{transform:rotate(180deg);-webkit-transform:rotate(180deg);-moz-transform:rotate(180deg);-webkit-transform:rotate(180deg);-o-transform:rotate(180deg); margin-bottom:0;}
.downOption_pull{ position:absolute; left:0; top:0.43rem; width:100%; overflow-y:scroll;}
.downOption_pull .list_box{background:#fff; display:none; overflow-y:scroll;}
.downOption_pull .list_box ul li{ display:block; border-bottom:0.01rem solid #dedede;}
.downOption_pull .list_box ul li a{display:block; padding:0.05rem 0.1rem; line-height:0.3rem; overflow:hidden;}
.downOption_pull .list_box ul li a .num{ display:inline-block; padding:0 0.08rem; border-radius:0.2rem; line-height:0.22rem; color:#999; vertical-align:middle; background:#e5e5e5;}
.downOption_pull .list_box ul li.on a{ color:#1ec0be;}
/*下拉选项筛选部分*/
.shaixuan_pull li{padding:0.05rem 0.1rem; line-height:0.3rem; overflow:hidden;}
.peisong_way{ border:0.01rem solid #1ec0be; border-radius:0.03rem; overflow:hidden;}
.peisong_way label{ display:inline-block; float:left; color:#1ec0be; padding:0 5px; border-right:0.01rem solid #1ec0be; position:relative;}
.peisong_way label:last-child{ border-right:none 0;}
.peisong_way label.on{ background:#1ec0be; color:#fff;}
.peisong_way label input{ position:absolute; opacity:0; filter:alpha(opacity=0); width:100%; height:100%; left:0; top:0;}
.tab_int{ display:inline-block; width:0.44rem; height:0.26rem; background:url(../images/tab_int.png) no-repeat center -0.26rem; background-size:100% auto; position:relative;}
.tab_int.on{ background-position:center top;}
.tab_int input{ position:absolute; opacity:0; filter:alpha(opacity=0); width:100%; height:100%; left:0; top:0;}
.downOption_pull .list_box ul .btn_box{ text-align:center;}
.shaixuan_pull li .btn{ display:inline-block; line-height:40px; width:49%; border-radius:0.05rem; font-size:0.14rem;}
.shaixuan_pull li .cancel{ margin-right:2%; border:0.01rem solid #dedede; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; background:#fff;}
.shaixuan_pull li .confirm{ background:#1ec0be; color:#fff; border:none 0;}
/*遮罩*/
.mask_bg{ position:fixed; width:100%; height:100%; display:none; z-index:100; background: rgba(0, 0, 0, 0.3); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#BF000000,endColorstr=#BF000000);}
.maskOne{ display:none; width:80%; position:fixed; z-index:1000; left:50%; margin-left:-40%; top:35%; margin-top:-1rem; background:#fff; overflow:hidden; border-radius:0.05rem;}
.maskOne .title{ background:#eeeeee; padding:0 0.1rem; line-height:0.4rem; font-size:0.16rem; color:#333;}
.maskOne .cont{ padding:0.1rem;}
/********************************/
.banner{ width:100%; height:auto; overflow:hidden; text-align:center; background:#fff;}

.form_list_box{ display:block; width:100%;}
.form_list_box li{ display:block; border-top:0.01rem solid #dedede; padding:0 0.1rem; background-color:#fff; min-height:0.4rem;}
.form_list_box li.last{border-bottom:0.01rem solid #dedede; margin-bottom:0.1rem;}

.long_btn_box{ padding:0.2rem 0.1rem;}
.long_btn{border-radius:0.05rem; height:0.4rem; width:100%; background-color:#1ec0be; border:none 0; font-size:0.16rem; color:#fff; display:block; text-align:center; line-height:0.4rem;}
.long_btn.bg_no{background-color:#fff; border:0.01rem solid #1ec0be; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; color:#1ec0be;}
.long_btn.graybg{ background-color:#dedede;}

.radioInt{ display:inline-block; width:0.26rem; height:0.26rem; border-radius:100%; overflow:hidden; background:url(../images/intIco.png) no-repeat center -0.78rem; background-size:100% auto;}
.radioInt input{opacity:0; filter:alpha(opacity=0);}
.radioInt.on{ background-position:center -1.04rem;}

.num_operate{overflow:hidden;}
.num_operate span{ border:0.01rem solid #1ec0be; vertical-align:middle; border-radius:0.03rem; height:0.22rem; width:0.22rem; display:inline-block; background-color:#fff; color:#1ec0be; line-height:0.22rem; text-align:center; font-size:0.22rem; cursor:pointer;}
.num_operate span.add{ background:#1ec0be; color:#fff;}
.num_operate input{display:inline-block; vertical-align:middle; width:0.3rem; height:0.24rem; background:none; border:none 0; text-align:center; font-size:0.16rem; line-height:0.24rem; color:#1ec0be;}
/*重复使用样式结束*/

/*登录密码修改开始*/
.loginModiy table{ text-align:left; font-size:0.14rem; border-top:0.01rem solid #dedede;}
.loginModiy table tr{ border-bottom:0.01rem solid #dedede; background:#fff;}
.loginModiy table th{ width:0.6rem; padding:0.1rem 0 0.1rem 0.1rem; line-height:0.3rem; font-weight:normal;}
.loginModiy table td{ padding:0.1rem 0.05rem; position:relative; height:0.3rem;}
.loginModiy table td input{ width:100%; height:0.3rem; border:none 0; background:none; font-size:0.14rem; color:#333;}
.loginModiy table td .get_yzm{position:absolute; right:0.05rem; top:0.05rem;}
.get_yzm{ padding:0 0.1rem; height:0.4rem; border-radius:0.05rem; background-color:#1ec0be; color:#fff; font-size:0.14rem; line-height:0.4rem; text-align:center;}
.get_yzm.on{ background-color:#dedede;}
.forgetPassword_box{ padding:0 0.1rem;}
/*注册协议*/
.userProtocol_check{ padding:0 0.1rem; line-height:0.24rem;}
.userProtocol_check input[type='checkbox']{ vertical-align:middle; margin-right:0.06rem;}
/*登录密码修改结束*/

/*个人中心首页开始*/
.mineHome_infor .infor{ background:#7ecef4; padding:0.2rem 0.1rem; text-align:center; font-size:0.18rem; line-height:0.34rem; color:#fff;}
.mineHome_infor .infor a{color:#fff;}
.mineHome_infor .infor .headX{ display:inline-block; width:0.6rem; height:0.6rem; border-radius:100%; overflow:hidden; background:#fff url(../images/headX.png) no-repeat center top; background-size:100% auto; margin-right:0.1rem; vertical-align:middle;}
.mineHome_infor .infor .headX img{ width:100%; height:100%;}
.mineHome_infor .infor em{content:''; display:inline-block; font-size:0; vertical-align:middle; width:0.10rem; height:0.10rem; border-bottom:0.02rem solid #fff;border-right:0.02rem solid #fff;transform:rotate(-45deg);-webkit-transform:rotate(-45deg);-moz-transform:rotate(-45deg);-webkit-transform:rotate(-45d eg);-o-transform:rotate(-45deg); margin-left:0.05rem;}
.mineHome_infor .list_box{ display:block; padding:0.05rem 0; border-bottom:0.01rem solid #dedede; background:#fff; overflow:hidden;}
.mineHome_infor .list{ display:block; float:left; width:33.33%; border-right:0.01rem solid #dedede; box-sizing:border-box; -moz-box-sizing:border-box; -webkit-box-sizing:border-box; text-align:center;}
.mineHome_infor .list:last-child{ border-right:none 0;}
.mineHome_infor .list p{ line-height:0.20rem; color:#333;}
.mineHome_infor .list p big{ font-size:0.14rem;}
.mineHome_list{line-height:0.4rem}
.mineHome_list p{ font-size:0.14rem;}
.mineHome_list p em{display:inline-block; font-size:0; vertical-align:middle; padding:0.12rem; background:url(../images/mineIco.png) no-repeat center top; background-size:100% auto; margin-right:0.05rem;}
.mineHome_list p em.ico_1{ background-position:center 0;}
.mineHome_list p em.ico_2{ background-position:center -0.24rem;}
.mineHome_list p em.ico_3{ background-position:center -0.48rem;}
.mineHome_list p em.ico_4{ background-position:center -0.72rem;}
.mineHome_list p em.ico_5{ background-position:center -0.96rem;}
.mineHome_list p em.ico_6{ background-position:center -1.2rem;}
.mineHome_list p em.ico_7{ background-position:center -1.44rem;}
.mineHome_list p em.ico_8{ background-position:center -1.68rem;}
.mineHome_list p em.ico_9{ background-position:center -1.92rem;}
.mineHome_list .num{ display:inline-block; color:#ff2121; padding:0 0.12rem; border-radius:0.2rem; border:0.01rem solid #ff2121; line-height:0.18rem; vertical-align:middle;}
.mineHome_list .linkIco{content:''; display:inline-block; font-size:0; vertical-align:middle; width:0.10rem; height:0.10rem; border-bottom:0.02rem solid #c9c9c9;border-right:0.02rem solid #c9c9c9;transform:rotate(-45deg);-webkit-transform:rotate(-45deg);-moz-transform:rotate(-45deg);-webkit-transform:rotate(-45d eg);-o-transform:rotate(-45deg); margin-left:0.05rem;}
/*个人中心首页结束*/

/*个人中心账户信息开始*/
.accountInfor .accountInfor_headX{ padding:0.05rem 0.1rem;}
.accountInfor .accountInfor_headX .img{ display:inline-block; width:0.4rem; height:0.4rem; border-radius:100%; overflow:hidden; background:#1ec0be url(../images/headX.png) no-repeat center bottom; background-size:100% auto; margin-right:0.1rem; vertical-align:middle;}
.accountInfor .accountInfor_headX .img img{ width:100%; height:100%;}
.accountInfor_headX_mask .btn{ display:block; font-size:0.14rem; line-height:0.3rem; margin-bottom:0.1rem; color:#999; text-align:center; position:relative;}
.accountInfor_headX_mask .btn input{opacity:0; filter:alpha(opacity=0); position:absolute;}
/*个人中心账户信息结束*/

/*订单列表页开始*/
.switchTab_box{ position:fixed; z-index:999; top:0.5rem; left:0; width:100%; padding:0.05rem 0; background:#fff; border-bottom:0.01rem solid #dedede; text-align:center; overflow:hidden;}
.switchTab{ display:inline-block; padding:0.02rem; background:#1ec0be; border-radius:0.03rem;}
.switchTab .link{ display:inline-block; font-size:0.14rem; line-height:0.30rem; width:0.8rem; color:#fff; border-radius:0.03rem;}
.switchTab .link.on{ background:#fff; color:#1ec0be;}
/*订单列表*/
.order_list{ display:block; background:#fff; border-bottom:0.01rem solid #dedede; padding:0.1rem; position:relative;}
.order_list>.fl{ width:70%;}
.order_list .state{ font-size:0.14rem; line-height:0.2rem; margin-bottom:0.05rem;}
.order_list .nr{ overflow:hidden;}
.order_list .img{ width:0.6rem; height:0.6rem;}
.order_list .img img{ width:100%; height:100%;}
.order_list .wz{ margin-left:0.7rem;}
.order_list .wz p{ font-size:0.14rem; line-height:0.24rem;}
.order_list .del_btn{display:inline-block; font-size:0; vertical-align:middle; padding:0.12rem; background:url(../images/headerIco.png) no-repeat center -1.2rem; background-size:100% auto;}
.order_list .pub_btn{ position:absolute; right:0.1rem; top:0.42rem; padding:0 0.06rem; min-width:0.54rem; text-align:center;}
/*订单无*/
.youhui_no{ margin:0.3rem 0; text-align:center; line-height:0.3rem;}
.youhui_no .pub_btn{ width:60%; height:0.4rem; line-height:0.4rem; font-size:0.16rem; margin:0.1rem auto;}
.youhui_no h2{ font-weight:normal; font-size:0.18rem;}
.youhui_no .iconBg{width:0.8rem; height:0.8rem; margin:0.1rem 0; display:inline-block; border-radius:0.8rem; text-align:center;}
.youhui_no .iconBg i{ display:inline-block; width:0.8rem; height:0.8rem; background:url(../images/ico.png) no-repeat; background-size:100% auto; margin:0.1rem;}
.youhui_no .iconBg.error{ width:1.2rem; height:1.2rem;}
.youhui_no .iconBg.error i{display:inline-block; width:1.2rem; height:1.2rem; background:url(../images/ico.png) no-repeat; background-size:100% auto; margin:0.1rem;}
.youhui_no .iconBg i.ico1{ background-position:0 0;}
.youhui_no .iconBg i.ico3{ background-position:0 -1.6rem;}
.youhui_no .iconBg i.ico4{ background-position:0 -2.4rem;}
.youhui_no .iconBg i.ico5{ background-position:0 -3.2rem;}
.youhui_no .iconBg i.ico6{ background-position:0 -4rem;}
.youhui_no .iconBg.error i.ico2{ background-position:0 -1.2rem;}
/*订单列表页结束*/

/*订单地址修或新增改表单开始*/
.orderAddr_list{ padding:0.05rem 0; border-bottom:0.01rem solid #dedede; background:#fff;}
.orderAddr_list>.fl{ width:0.4rem; height:0.4rem; border-right:0.01rem solid #dedede; text-align:center; line-height:0.4rem;}
.orderAddr_list>.fl i{ display:inline-block; font-size:0; vertical-align:middle; padding:0.12rem; background:url(../images/intIco.png) no-repeat center top; background-size:100% auto;}
.orderAddr_list>.fl i.ico_1{ background-position:center 0;}
.orderAddr_list>.fl i.ico_2{ background-position:center -0.24rem;}
.orderAddr_list>.fl i.ico_3{ background-position:center -0.48rem;}
.orderAddr_list>.fl i.ico_4{ background-position:center -1.92rem;}
.orderAddr_list>.fl i.ico_5{ background-position:center -2.16rem;}
.orderAddr_list>.fl i.ico_6{ background-position:center -2.4rem;}
.orderAddr_list .orderAddr_int{ margin-left:0.5rem; position:relative;}
.orderAddr_list .orderAddr_int_default{ margin-left:0.7rem;}
.orderAddr_list .orderAddr_int_default:after{ position:absolute; top:50%; margin-top:-0.03rem; right:0.1rem; content:''; border-left: 0.05rem solid transparent; border-right: 0.05rem solid transparent; border-top: 0.06rem solid #1ec0be;}
.orderAddr_int_default_mask label{ display:block; font-size:0.14rem; line-height:0.34rem; border-bottom:0.01rem dashed #dedede;}
.orderAddr_int_default_mask label input{ vertical-align:middle; margin-right:0.05rem;}

.orderAddr_list .orderAddr_int .get_yzm{position: absolute; right: 0.05rem; top: 0rem;}
.orderAddr_list .orderAddr_int input{ width:100%; height:0.4rem; border:none 0; background:none; font-size:0.14rem; color:#333;}
.orderAddr_list .orderAddr_int .link{ position:absolute; right:0.1rem; top:50%; margin-top:-0.2rem; display:inline-block; width:0.4rem; height:0.4rem; background:#eeeeee; vertical-align:middle; line-height:0.4rem;}
.orderAddr_list .orderAddr_int .link .linkIco{content:''; display:inline-block; font-size:0; line-height:0; width:0.12rem; height:0.12rem; border-bottom:0.02rem solid #1ec0be;border-right:0.02rem solid #1ec0be;transform:rotate(-45deg);-webkit-transform:rotate(-45deg);-moz-transform:rotate(-45deg); margin-left:0.08rem; margin-top:0.12rem;}
.addrDel_mask p{ font-size:0.14rem; line-height:0.24rem;}
.addrDel_mask .btn_box{ text-align:center; margin:0.1rem 0;}
.addrDel_mask .btn_box .pub_btn{ padding:0 0.15rem; margin:0 0.05rem;}
/*订单地址修或新增-定位*/
.searchBox{ width:65%; margin:0 auto;}
.searchBox input{ width:100%; border:none 0; border-bottom:0.01rem solid #f0f0f0; background:none; height:0.34rem; line-height:0.34rem; -webkit-appearance:normal; appearance:normal; color:#fff;}
.searchBox input:-moz-placeholder{color: #f0f0f0; opacity:0.6; filter:alpha(opacity=60);}  
.searchBox input::-webkit-input-placeholder{color:#f0f0f0; opacity:0.6; filter:alpha(opacity=60);}
.orderAddr_page .map{ width:100%; height:auto; overflow:hidden;}
.orderAddr_page .map img{ width:100%;}
.orderAddr_map_list{ display:block; border-bottom:0.01rem solid #dedede; padding:0.1rem; overflow:hidden;}
.orderAddr_map_list .ico{width:0.24rem; height:0.24rem; background:url(../images/intIco.png) no-repeat left -1.44rem; background-size:100% auto; margin-top:0.1rem; font-size:0; vertical-align:middle;}
.orderAddr_map_list .wz{ margin-left:0.30rem; font-size:0.14rem; line-height:0.24rem;}
/*订单地址修改或新增表单结束*/

/*订单详情-进度开始*/
.order_progress_infor{border-bottom:0.01rem solid #dedede; background:#fff; padding:0.05rem 0.1rem;}
.order_progress_infor p{ line-height:0.24rem;}
.order_progress_infor .btn{ display:inline-block; border:0.01rem solid #ff2121; font-size:0.14rem; line-height:0.3rem; color:#ff2121; padding:0 0.06rem; border-radius:0.03rem; margin-top:0.06rem;}
.order_progress{ padding:0.1rem; background:#fff; border-bottom:0.01rem solid #dedede; border-top:0.01rem solid #dedede;}
.order_progress ul{}
.order_progress_list{ padding:0.1rem 0 0; position:relative; height:auto;}
.order_progress_list .ico{ display:block; width:0.30rem; height:0.30rem; background:#fff url(../images/order_progress.png) no-repeat left top; background-size:0.6rem auto; margin-right:0.1rem; font-size:0; vertical-align:middle; position:relative; z-index:1;}
.order_progress_list .ico.ico_1{ background-position:left 0rem;}
.order_progress_list .ico.ico_2{ background-position:left -0.3rem;}
.order_progress_list .ico.ico_3{ background-position:left -0.6rem;}
.order_progress_list .ico.ico_4{ background-position:left -0.9rem;}
.order_progress_list .ico.ico_5{ background-position:left -1.2rem;}
.order_progress_list .ico.ico_6{ background-position:left -1.5rem;}
.order_progress_list .ico.ico_7{ background-position:left -1.8rem;}
.order_progress_list .ico.ico_1.on{ background-position:right 0rem;}
.order_progress_list .ico.ico_2.on{ background-position:right -0.3rem;}
.order_progress_list .ico.ico_3.on{ background-position:right -0.6rem;}
.order_progress_list .ico.ico_4.on{ background-position:right -0.9rem;}
.order_progress_list .ico.ico_5.on{ background-position:right -1.2rem;}
.order_progress_list .ico.ico_6.on{ background-position:right -1.5rem;}
.order_progress_list .ico.ico_7.on{ background-position:right -1.8rem;}
.order_progress_list .wz{ margin-left:0.4rem; padding:0 0 0.1rem; border-bottom:0.01rem solid #dedede; font-size:0.14rem; line-height:0.24rem; position:relative;}
.order_progress_list .wz .long_xian,.order_progress_list .wz .xian{ display:block; width:0.01rem; position:absolute; z-index:0; left:-0.25rem; background: url(../images/rept_y1.png) repeat-y left top; background-size:0.01rem auto;}
.order_progress_list .wz .long_xian{ height:100%; top:0;}
.order_progress_list .wz .xian{ height:0.11rem; top:-0.11rem;}
.order_progress_list:last-child .wz .long_xian{ display:none;}
.order_progress_list:first-child .wz .xian{ display:none;}

.order_progress_list:last-child .wz{ border-bottom:none 0;}
.order_progress_list .tel{width:0.30rem; height:0.30rem; background:#fff url(../images/telIco.png) no-repeat left top; background-size:100% auto; font-size:0; vertical-align:middle; position:absolute; right:0; bottom:0.04rem;}
.ord_tousu{ text-align:center; padding:0.1rem; overflow:hidden; font-size:0.14rem;}
.ord_tousu a{ display:block;}
