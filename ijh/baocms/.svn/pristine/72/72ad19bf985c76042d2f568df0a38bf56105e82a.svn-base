                <div class="register_intw register_intBox">
                    <input type="text" class="radius3 tuanfabu_int register_intw" value="请输入公司地址" onblur="if (!value) { value = defaultValue; this.style.color = '#999' }" onclick="if (value == defaultValue) { value = ''; this.style.color = '#000' }" />
                    <div class="left int_tsk" style="display:none;">
                    	<p class="tsk_error"></p>
                    </div>
                    <div class="left int_tsk">
                        <p class="tsk_error tsk_correct">&nbsp;</p>
                    </div>
                </div>
            </td>
        </tr>
        <tr>
            <td><p class="register_p">公司电话</p></td>
            <td>
                <div class="register_intw register_intBox">
                    <input type="text" class="radius3 tuanfabu_int register_intw" value="请输入公司电话" onblur="if (!value) { value = defaultValue; this.style.color = '#999' }" onclick="if (value == defaultValue) { value = ''; this.style.color = '#000' }" />
