/*时间弹出层开始*/
.date_mask{ position:fixed; display:none; z-index:1000; width:100%; height:100%; overflow:scroll; bottom:0; left:0; background:rgba(0,0,0,0.3); filter:progid:DXImageTransform.Microsoft.gradient(startColorstr=#BF000000,endColorstr=#BF000000);}
.date_mask .cont{ width:100%; position:absolute; bottom:0; overflow:scroll; max-height:100%; height:auto;}

.week_box{ background:#f5f5f5; padding:0.05rem 0.3rem; position:relative;}
.week_list{ display:block; width:100%; height:0.36rem; overflow:hidden;}
.week_list li{ display:block; float:left; width:20%; text-align:center; font-size:0.14rem; line-height:0.18rem;}
.week_list li.on p{ color:#901872;}
.week_box .prev{content:''; width:0.12rem; height:0.12rem; border-bottom:0.02rem solid #901872;border-right:0.02rem solid #901872;transform:rotate(-45deg);-webkit-transform:rotate(-45deg);-moz-transform:rotate(-45deg);-webkit-transform:rotate(-45deg);-o-transform:rotate(-45deg); position:absolute; right:0.1rem; top:0.15rem;}
.week_box .next{content:''; width:0.12rem; height:0.12rem; border-bottom:0.02rem solid #901872;border-left:0.02rem solid #901872;transform:rotate(45deg);-webkit-transform:rotate(45deg);-moz-transform:rotate(45deg);-webkit-transform:rotate(45deg);-o-transform:rotate(-45deg); position:absolute; left:0.1rem; top:0.15rem;}

.week_time_box{ padding:0.1rem 0.1rem 0; background:#fff;}
.week_time{ overflow:hidden; display:none;}
.week_time ul li{display:block; float:left; width:22%; margin-right:4%; text-align:center; font-size:0.14rem; line-height:0.24rem; height:0.24rem; color:#666; background:#d8d8d8; border-radius:0.03rem; margin-bottom:0.1rem;}
.week_time ul li:nth-child(4n){ margin-right:0;}
.week_time ul li.on{ background:#2cca77; color:#fff;}
/*时间弹出层结束*/
